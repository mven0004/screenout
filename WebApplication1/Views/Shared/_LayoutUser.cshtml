<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>ScreenOut.Team</title>
    <link rel="icon" href="../../img/screenout-logo.png" type="image/gif" sizes="16x16">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="padding-bottom:0px">
    <nav class="navbar navbar-dark navbar-expand-lg fixed-top py-2 bg-dark">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                <img src="../../img/screenout-logo.png" width="50" height="30" class="d-inline-block align-top" alt="">
                ScreenOut
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon "></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="@(ViewBag.CurrentPage == "Home" ? "nav-item active" : "nav-item")">
                        @Html.ActionLink("Home", "Index", "Home", null, htmlAttributes: new { @class = "nav-link" })
                    </li>

                    <!-- Dropdown -->
                    <li class="@(ViewBag.CurrentPage == "Research" ? "nav-item dropdown active" : "nav-item dropdown")">
                        <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                            Facts
                        </a>
                        <div class="dropdown-menu">
                            @Html.ActionLink("Screen usage and issues", "ScreenTime", "Home", null, new { @class = "dropdown-item" })
                            @Html.ActionLink("Video game and bullying", "GameTime", "Home", null, new { @class = "dropdown-item" })
                        </div>
                    </li>

                    <li class="@(ViewBag.CurrentPage == "Education" ? "nav-item active" : "nav-item")">
                        @Html.ActionLink("Developing habits", "Education", "Home", null, new { @class = "nav-link" })
                    </li>
                    <li class="@(ViewBag.CurrentPage == "MediaPlan" ? "nav-item active" : "nav-item")">
                        @Html.ActionLink("Screen time plan", "MediaPlan", "Home", null, new { @class = "nav-link" })
                    </li>
                    
                    @Html.Partial("_LoginPartial")
                </ul>
            </div>
        </div>
    </nav>

